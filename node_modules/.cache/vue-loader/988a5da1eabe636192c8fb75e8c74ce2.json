   WCol,\r\n    OnlineReview,\r\n  },\r\n})\r\nexport default class GridView extends Vue {\r\n  Grid: string = TITLE.Grid;\r\n\r\n  base: ApiEntity = base;\r\n\r\n  gutter: ApiEntity = gutter;\r\n\r\n  offset: ApiEntity = offset;\r\n\r\n  sort: ApiEntity = sort;\r\n\r\n  justify: ApiEntity = justify;\r\n\r\n  align: ApiEntity = align;\r\n\r\n  order: ApiEntity = order;\r\n\r\n  media: ApiEntity = media;\r\n\r\n  rowProps: PropsEntity[] = rowProps;\r\n\r\n  colProps: PropsEntity[] = colProps;\r\n}\r\n",{"version":3,"sources":["Grid.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;;AAEA;;AAEA;;AAEA;;AAEA;;AAEA;;AAEA;;AAEA;;AAEA;AACA","file":"Grid.vue","sourceRoot":"src/views/demo","sourcesContent":["<template>\r\n  <div>\r\n    <h1>{{ Grid }}</h1>\r\n    <p>24 栅格系统。</p>\r\n    <h2>概述</h2>\r\n    <p>布局的栅格化系统，我们是基于行（row）和列（col）来定义信息区块的外部框架，以保证页面的每个区域能够稳健地排布起来。下面简单介绍一下它的工作原理：</p>\r\n    <ul>\r\n      <li>通过 <code>row</code> 在水平方向建立一组 <code>column</code>（简写 col）</li>\r\n      <li>你的内容应当放置于 <code>col</code> 内，并且，只有 <code>col</code> 可以作为 <code>row</code> 的直接元素</li>\r\n      <li>栅格系统中的列是指 1 到 24 的值来表示其跨越的范围。例如，三个等宽的列可以使用 <code>.col-8</code> 来创建</li>\r\n      <li>如果一个 <code>row</code> 中的col总和超过 24，那么多余的 <code>col</code> 会作为一个整体另起一行排列</li>\r\n    </ul>\r\n    <h2>代码演示</h2>\r\n    <WRow :gutter=\"16\">\r\n      <WCol :span=\"24\">\r\n        <OnlineReview\r\n          :temCode=\"base.code\"\r\n          :title=\"base.title\"\r\n          :desc=\"base.desc\"\r\n        ></OnlineReview>\r\n        <OnlineReview\r\n          :temCode=\"gutter.code\"\r\n          :title=\"gutter.title\"\r\n          :desc=\"gutter.desc\"\r\n        ></OnlineReview>\r\n        <OnlineReview\r\n          :temCode=\"offset.code\"\r\n          :title=\"offset.title\"\r\n          :desc=\"offset.desc\"\r\n        ></OnlineReview>\r\n        <OnlineReview\r\n          :temCode=\"sort.code\"\r\n          :title=\"sort.title\"\r\n          :desc=\"sort.desc\"\r\n        ></OnlineReview>\r\n        <OnlineReview\r\n          :temCode=\"justify.code\"\r\n          :title=\"justify.title\"\r\n          :desc=\"justify.desc\"\r\n        ></OnlineReview>\r\n        <OnlineReview\r\n          :temCode=\"align.code\"\r\n          :title=\"align.title\"\r\n          :desc=\"align.desc\"\r\n        ></OnlineReview>\r\n        <OnlineReview\r\n          :temCode=\"order.code\"\r\n          :title=\"order.title\"\r\n          :desc=\"order.desc\"\r\n        ></OnlineReview>\r\n        <OnlineReview\r\n          :temCode=\"media.code\"\r\n          :title=\"media.title\"\r\n          :desc=\"media.desc\"\r\n        ></OnlineReview>\r\n      </WCol>\r\n    </WRow>\r\n    <h2>Row API</h2>\r\n    <ApiTable :data=\"rowProps\"></ApiTable>\r\n    <h2>Col API</h2>\r\n    <ApiTable :data=\"colProps\"></ApiTable>\r\n  </div>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport { Component, Vue } from 'vue-property-decorator';\r\nimport ApiTable from '@/views/components/ApiTable.vue';\r\nimport WRow from '@/components/grid/Row.vue';\r\nimport WCol from '@/components/grid/Col.vue';\r\nimport OnlineReview from '@/views/components/OnlineReview.vue';\r\nimport { ApiEntity } from '@/views/entity/demoentity';\r\nimport base from '@/views/code/grid/base';\r\nimport gutter from '@/views/code/grid/gutter';\r\nimport offset from '@/views/code/grid/offset';\r\nimport sort from '@/views/code/grid/sort';\r\nimport justify from '@/views/code/grid/justify';\r\nimport align from '@/views/code/grid/align';\r\nimport order from '@/views/code/grid/order';\r\nimport media from '@/views/code/grid/media';\r\nimport { rowProps, colProps } from '@/views/api/grid';\r\nimport { PropsEntity } from '@/views/entity/apientity';\r\nimport { TITLE } from '@/views/common/constant';\r\n\r\n@Component({\r\n  components: {\r\n    ApiTable,\r\n    WRow,\r\n    WCol,\r\n    OnlineReview,\r\n  },\r\n})\r\nexport default class GridView extends Vue {\r\n  Grid: string = TITLE.Grid;\r\n\r\n  base: ApiEntity = base;\r\n\r\n  gutter: ApiEntity = gutter;\r\n\r\n  offset: ApiEntity = offset;\r\n\r\n  sort: ApiEntity = sort;\r\n\r\n  justify: ApiEntity = justify;\r\n\r\n  align: ApiEntity = align;\r\n\r\n  order: ApiEntity = order;\r\n\r\n  media: ApiEntity = media;\r\n\r\n  rowProps: PropsEntity[] = rowProps;\r\n\r\n  colProps: PropsEntity[] = colProps;\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n@import '~assets/style/grid.scss';\r\n</style>\r\n"]}]}