yle lang=\"scss\">\r\n.affix {\r\n  &-box {\r\n    width: 1100px;\r\n    margin: 0 auto;\r\n    display: flex;\r\n    justify-content: space-between;\r\n\r\n    &-left {\r\n      height: 2000px;\r\n      width: 760px;\r\n      margin-right: 20px;\r\n      background: #f00;\r\n      padding: 32px 20px 24px;\r\n      border-radius: 8px;\r\n      box-shadow: 0 0 16px 0 rgba(0, 0, 0, 0.06);\r\n    }\r\n\r\n    &-right {\r\n      position: relative;\r\n      width: 320px;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}