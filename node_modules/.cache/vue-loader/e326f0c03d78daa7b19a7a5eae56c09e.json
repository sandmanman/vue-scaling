\n    <h2>代码演示</h2>\r\n    <WRow :gutter=\"16\">\r\n      <WCol :span=\"12\">\r\n        <OnlineReview\r\n          :temCode=\"base.code\"\r\n          :jsCode=\"base.js\"\r\n          :title=\"base.title\"\r\n          :desc=\"base.desc\"\r\n          iframeHeight=\"200px\"\r\n        ></OnlineReview>\r\n        <OnlineReview\r\n          :temCode=\"diy.code\"\r\n          :jsCode=\"diy.js\"\r\n          :title=\"diy.title\"\r\n          :desc=\"diy.desc\"\r\n          iframeHeight=\"200px\"\r\n        ></OnlineReview>\r\n        <OnlineReview\r\n          :temCode=\"disabled.code\"\r\n          :jsCode=\"disabled.js\"\r\n          :title=\"disabled.title\"\r\n          :desc=\"disabled.desc\"\r\n          iframeHeight=\"200px\"\r\n        ></OnlineReview>\r\n        <OnlineReview\r\n          :temCode=\"render.code\"\r\n          :jsCode=\"render.js\"\r\n          :title=\"render.title\"\r\n          :desc=\"render.desc\"\r\n          iframeHeight=\"200px\"\r\n        ></OnlineReview>\r\n      </WCol>\r\n      <WCol :span=\"12\">\r\n        <OnlineReview\r\n          :temCode=\"def.code\"\r\n          :jsCode=\"def.js\"\r\n          :title=\"def.title\"\r\n          :desc=\"def.desc\"\r\n          iframeHeight=\"200px\"\r\n        ></OnlineReview>\r\n        <OnlineReview\r\n          :temCode=\"size.code\"\r\n          :jsCode=\"size.js\"\r\n          :title=\"size.title\"\r\n          :desc=\"size.desc\"\r\n          iframeHeight=\"200px\"\r\n        ></OnlineReview>\r\n        <OnlineReview\r\n          :temCode=\"search.code\"\r\n          :jsCode=\"search.js\"\r\n          :title=\"search.title\"\r\n          :desc=\"search.desc\"\r\n          iframeHeight=\"200px\"\r\n        ></OnlineReview>\r\n        <OnlineReview\r\n          :temCode=\"loading.code\"\r\n          :jsCode=\"loading.js\"\r\n          :title=\"loading.title\"\r\n          :desc=\"loading.desc\"\r\n          iframeHeight=\"200px\"\r\n        ></OnlineReview>\r\n      </WCol>\r\n    </WRow>\